{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "[%(levelname)s]: %(asctime)s - %(name)s:%(funcName)s:%(lineno)d - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "default"
    },
    "rotating_file": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "DEBUG",
      "formatter": "default",
      "filename": "spam.log",
      "maxBytes": 10485760,
      "backupCount": 20
    }
  },
  "loggers": {
    "": {
      "handlers": ["console", "rotating_file"],
      "level": "DEBUG"
    }
  }
}
